{
  "name": "react-scheduler",
  "version": "0.0.1",
  "description": "Scheduler | Calendar",
  "author": {
    "name": "Filipe Marins",
    "email": "filipemarins123@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/filipemarins/react-scheduler.git"
  },
  "license": "MIT",
  "main": "dist/react-scheduler.cjs.js",
  "module": "dist/react-scheduler.esm.js",
  "style": "dist/css/react-scheduler.css",
  "files": [
    "lib/",
    "dist/",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "scheduler",
    "react-component",
    "react",
    "events",
    "appointments",
    "calendar",
    "react big calendar"
  ],
  "scripts": {
    "clean:examples": "rimraf examples/static",
    "sass": "node-sass src/sass/styles.scss ./dist/css/react-scheduler.css",
    "build:css": "npm run sass",
    "build": "yarn rollup -c && yarn build:css",
    "build:examples": "npm run clean:examples && webpack --config examples/webpack.config.js",
    "examples": "npm run clean:examples && webpack-dev-server --config examples/webpack.config.js --mode development",
    "lint": "eslint src test stories",
    "lint-staged": "npx lint-staged",
    "storybook": "start-storybook -p 9002",
    "test": "jest",
    "tdd": "jest --watch",
    "prepublishOnly": "npm run build",
    "prebuild": "rimraf dist"
  },
